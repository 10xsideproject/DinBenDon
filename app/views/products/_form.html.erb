<%= form_for(product) do |f| %>
  <div>
    <%= f.label :name , '店名' %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :price , '價格' %>
    <%= f.number_field :price %>
  </div>
  <div>
    <%= f.label :store_id , '價格' %>
    <%= f.number_field :store_id %>
  </div>
  <%= f.submit '送出' %>
<% end %>
  