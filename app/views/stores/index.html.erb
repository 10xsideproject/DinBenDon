<h1>商家列表 <span><%= link_to "新增商家", new_store_path %></span></h1>

<ul>
  <% @stores.each do |store| %>
    <li>
      <%= link_to store.name, store %>
      <%= link_to "編輯商家", edit_store_path(store.id) %>
      <%= link_to "刪除商家", store, method: "delete", data: {confirm: "確定刪除嗎？"} %>
    </li>
  <% end %>  
</ul>
