<%= form_with model: store do |form| %>
  <div>
    <%= form.label :name, "店名" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :address, "地址" %>
    <%= form.text_field :address %>
  </div>
  <div>
    <%= form.label :tel, "電話" %>
    <%= form.text_field :tel %>
  </div>
  <%= f.submit "送出" %>
<% end %>
  